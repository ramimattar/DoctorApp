{
  "expo": {
    "name": "DoctorPatientApp",
    "slug": "DoctorPatientApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/doctor-logo.png",
    "scheme": "doctorpatientapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/doctor-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.rami.matar.DoctorPatientApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/stethoscope-loading.png'",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ebbdd15c-4b5f-433e-9252-7dfb7ae63827"
      }
    }
  }
}
